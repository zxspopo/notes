<script type="text/javascript">
!function(){
    $w=$(".channel-wrapper").width();
    $(".channel-right span.channel-close").on("click",function(){
        $(".channel-wrapper").animate({
            left: -$w
        }, 500,function(){
            $(".channel-min").animate({
                left: "-10px"
            }, 100);
        } );
    })
    $(".channel-min").on("click",function(){
        $(".channel-min").animate({
            left: "-254px"
        }, 100,function() {
            $(".channel-wrapper").animate({
                left: 0
            }, 500)
        });
    })
    function emptyF($this){
		if($($this).css("color")=="rgb(255, 0, 0)"){
			$($this).val($($this).attr("data")).css({"color":"#666","border-color":"#ccc"});
		}
	}
	$("input").focus(function(){
		emptyF($(this))
	});
}();
$("#bookOrder").click(function(){
	if($("input[name=mobile]").css("color")=="rgb(255, 0, 0)" || $("input[name=name]").css("color")=="rgb(255, 0, 0)"){
		return ;
	}
	/*手机号码验证*/
	var mobile = $("input[name=mobile]").val();
	var mobile86 =  /^[1]\d{10}$/g, //中国大陆手机
	mobile852 = /^[1-9]\d{7}$/g; //中国香港手机
	if(mobile==""){
		$("input[name=mobile]").val("请填写您的手机号").css({"color":"red","border-color":"red"});
		return ;
	}else if(!mobile86.test(mobile)){
		$("input[name=mobile]").val("请填写真实手机号").css({"color":"red","border-color":"red"}).attr("data",mobile);
		return ;
	}else{
		$("input[name=mobile]").css({"color":"#666","border-color":"#ccc"});
	}
	/*您的称呼验证*/
	var name = $("input[name=name]").val();
	var counter=name.length;
	if(name==""){
		$("input[name=name]").val("请填写您的称呼").css({"color":"red","border-color":"red"});
		return ;
	}else if(counter>10){
		$("input[name=name]").val("不超过10个字").css({"color":"red","border-color":"red"}).attr("data",name);
		return ;
	}else{
		$("input[name=name]").css({"color":"#666","border-color":"#ccc"});
	}
	$bookOrder.init("JF_WEB_INDEX",mobile,name,"");
});
