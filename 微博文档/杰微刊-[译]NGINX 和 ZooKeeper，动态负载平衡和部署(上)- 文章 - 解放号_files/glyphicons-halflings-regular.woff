function getNewPutOrders(logintime){
	setInterval(
		function(){ 
			$.ajax({
				url: "/getneworders",
			    type: "POST",
			    dataType: "json",
			    data:logintime,
			    success:function(json){
				    var data = json.rows;
				    var _html = "";
				    for(var i=0;i<data.length;i++){
				    	_html += "<li><span class='time txtellipsis'>"+data[i].timeTip+"发布</span><span class='worktype txtellipsis'>【"+data[i].tech_direction+"】</span>";
					    _html += "<a target='_blank' href='https://www.jointforce.com/jfportal/all/detail/"+data[i].oid+"' class='green ordername'>"+data[i].onameTip+"</a>";
					    _html += "<span class='price'>￥";
						if(data[i].order_type=="4"){
							_html += "";
						}else{
							_html += "";
						}
					    _html += "</span></li>";
					}
				    $("#blist ul").html();
				}
			});
	},1000*60*5);//每隔5分钟请求最新数据
}
